        "/api/amina/add_feedback": {
            "post": {
                "summary": "Добавление отзыва",
                "description": "Метод позволяет пользователю добавить отзыв с текстом и изображением.",
                "operationId": "a3bf0d1e80bae4baf03bbbaaa16c9f95",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "creator",
                                    "region",
                                    "fio",
                                    "email",
                                    "text",
                                    "image"
                                ],
                                "properties": {
                                    "creator": {
                                        "type": "string",
                                        "example": "Создатель"
                                    },
                                    "organization": {
                                        "type": "string",
                                        "example": "Организация"
                                    },
                                    "job_title": {
                                        "type": "string",
                                        "example": "Должность"
                                    },
                                    "region": {
                                        "type": "string",
                                        "example": "Казань"
                                    },
                                    "fio": {
                                        "type": "string",
                                        "example": "Иванов Иван Иванович"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "user@example.com"
                                    },
                                    "text": {
                                        "type": "string",
                                        "example": "Текст отзыва"
                                    },
                                    "image": {
                                        "description": "Картинка",
                                        "type": "string",
                                        "format": "binary"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Отзыв успешно добавлен",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "422": {
                        "description": "Ошибка валидации",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "500": {
                        "description": "Ошибка при добавлении отзыва",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                }
            }
        },
